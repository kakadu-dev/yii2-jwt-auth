{
  "name": "kakadu-dev/yii2-jwt-auth",
  "description": "Extension provide JWT auth for Yii2",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "jwt",
    "auth",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ярмолюк Михаил",
      "email": "lukomi@mail.ru"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2.0.15",
    "firebase/php-jwt": "^5.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Kakadu\\Yii2JwtAuth\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  }
}